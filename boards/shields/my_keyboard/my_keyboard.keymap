
// config/my_keyboard.keymap
#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";
        label = "Default";

        default_layer {
            bindings = <
                &kp Q &kp W &kp E
                &kp A &kp S &kp D
                &kp Z &kp X &kp C
              
            >;
        };
    };
};
